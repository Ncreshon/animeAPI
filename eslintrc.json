{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended", "prettier"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
      "no-console": "error",
      "prettier/prettier": "error"
    }
  }